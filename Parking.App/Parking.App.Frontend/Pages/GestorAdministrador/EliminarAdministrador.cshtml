@page
@model Parking.App.Frontend.EliminarAdministradorModel
@{
}
<div class="center">
<h3>Eliminar Administrador - @Model.administrador.Nombre</h3>
<form method="post" class="mt-3">
    <input hidden asp-for="administrador.Identificacion">
    <input hidden asp-for="administrador.IdAdministrador">

    <br>       

        <li>Nombre:  @Model.administrador.Nombre</li><br>  
        <li>Identificacion:  @Model.administrador.Identificacion</li><br>        
        <li>Telefono:  @Model.administrador.Telefono</li><br>        
        <li>Direccion:  @Model.administrador.Direccion</li><br>        
        <li>Email:  @Model.administrador.Email</li><br>        
        <li>Contrasena:  @Model.administrador.Contrasena</li><br>        
        <li>Fecha Nacimiento:  @Model.administrador.Fecha_nacimiento</li><br>        
    <br><br>
    <div>
        <button type="submit" class="btn btn-primary">Eliminar</button>
        <a asp-page="./ListarAdministrador" class="btn btn-primary">Cancelar</a>
    </div>
</form>
</div>