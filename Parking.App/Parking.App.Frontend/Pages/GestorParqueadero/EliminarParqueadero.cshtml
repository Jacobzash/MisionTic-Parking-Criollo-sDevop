@page
@model Parking.App.Frontend.EliminarParqueaderoModel
@{
}
<div class="center">
<h3>Eliminar Parqueadero @Model.parqueadero.Espacio</h3>
<form method="post" class="mt-3">
   
    <input hidden asp-for="parqueadero.Id">
    <div class="form-group row">
        <button type="submit" class="btn btn-primary">Eliminar</button>
        <a asp-page="./ListarParqueadero" class="btn btn-primary">Cancelar</a>
    </div>
</form>
</div>